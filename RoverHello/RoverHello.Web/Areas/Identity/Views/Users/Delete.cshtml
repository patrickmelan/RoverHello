@model UserViewModel

@{
	Layout = null;
    ViewData["Title"] = "Delete User";
}


        <dl class="dl-horizontal">
            <dd>
                Email: @Html.DisplayFor(model => model.Email)
            </dd>
            <dt>
                First Name: @Html.DisplayFor(model => model.FirstName)
            </dt>
            <dd>
                Last Name: @Html.DisplayFor(model => model.LastName)
            </dd>
            <dd>
                Role: @Html.DisplayFor(model => model.Roles)
            </dd>
            <dt>
                Grade: @Html.DisplayFor(model => model.Grade)
            </dt>
            <dd>
                Points: @Html.DisplayFor(model => model.Points)
            </dd>
        </dl>

<form asp-action="Delete" id="deleteForm">
	<input type="hidden" asp-for="Id"/>
</form>

